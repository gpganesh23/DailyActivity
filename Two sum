class Solution {
    public int[] twoSum(int[] nums, int target) {
        int[] ans=new int[2];
        for(int l=0;l<nums.length-1;l++){
        for(int r=l+1;r<nums.length;r++){
            if(nums[l]+nums[r]==target){
                ans[0]=l;
                ans[1]=r;
                break;
            }   
            }
        }
        return ans;
    }
}
